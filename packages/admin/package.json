{
	"name": "@bank/admin",
	"version": "0.1.0",
	"private": true,
	"prisma": {
		"schema": "../db/prisma/schema.prisma"
	},
	"scripts": {
		"build": "tsc --build tsconfig.json",
		"clean": "rimraf dist && rimraf .cache",
		"prod": "TS_NODE_PROJECT=./tsconfig.prod.json node -r tsconfig-paths/register ./dist/main.js",
		"dev": "ts-node-dev -r tsconfig-paths/register --project ./tsconfig.json --debug --no-notify --respawn --ignore /.yarn/,/dist/,node_modules --transpile-only ./src/main.ts",
		"lint": "eslint \"src/**/*.ts\"",
		"prisma:generate": "prisma generate",
		"migrate:dev": "npx prisma migrate dev",
		"migrate:deploy": "npx prisma migrate deploy",
		"migrate:full": "yarn migrate:dev && yarn migrate:deploy",
		"lint:fix": "eslint ./src --ext .ts --fix",
		"typecheck": "yarn build"
	},
	"dependencies": {
		"@adminjs/design-system": "^3.1.8",
		"@adminjs/express": "^5.1.0",
		"@adminjs/nestjs": "^5.1.1",
		"@adminjs/prisma": "^3.0.1",
		"@nestjs/common": "^9.3.12",
		"@nestjs/core": "^9.3.12",
		"@nestjs/jwt": "^10.0.3",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.3.12",
		"@ntegral/nestjs-sentry": "^4.0.0",
		"@prisma/client": "^4.12.0",
		"@types/multer": "^1.4.7",
		"adminjs": "^6.8.7",
		"apollo-server": "3.12.0",
		"apollo-server-core": "^3.12.0",
		"app": "link:./src/app",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cloudflare": "^2.9.1",
		"const": "link:./src/const",
		"dataloader": "^2.2.2",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"express-formidable": "^1.2.0",
		"express-session": "^1.17.3",
		"modules": "link:./src/modules",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"prisma": "^4.12.0",
		"react": "^18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.0",
		"source-map-support": "^0.5.21",
		"styled-components": "^5.3.9",
		"tsconfig-paths": "^4.2.0",
		"utils": "link:./src/utils",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/express-formidable": "^1.2.0",
		"@types/express-session": "^1.17.7",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.15.11",
		"@types/passport": "^1.0.12",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@types/styled-components": "^5.1.26",
		"@types/uuid": "^9.0.1",
		"rimraf": "^4.4.1",
		"ts-node": "10.9.1",
		"ts-node-dev": "2.0.0",
		"typescript": "4.8.3"
	}
}
